   "source": [
    "# 名前と年齢を変数として定義\n",
    "name = \"白井かおり\"\n",
    "current_age = 35  # 現在の年齢\n",
    "\n",
    "# 来年の年齢を計算\n",
    "next_year_age = current_age + 1\n",
    "\n",
    "# 結果を表示\n",
    "print(\"はじめまして、\", name, \"と申します。\")\n",
    "print(\"年齢は\", current_age, \"歳です。来年は\", next_year_age, \"歳になります。\")\n",
    "print(\"よろしくお願いします。\")"
   ]
  }
