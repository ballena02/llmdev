   "source": [
    "# 元金と年利を入力\n",
    "principal = float(input(\"元金を入力してください: \"))\n",
    "rate_percent = float(input(\"年利を入力してください: \"))\n",
    "\n",
    "# 年利（%）を小数に変換\n",
    "rate = rate_percent / 100\n",
    "\n",
    "# 複利で10年間計算（繰り返し構文を使用）\n",
    "amount = principal\n",
    "for _ in range(10):\n",
    "    amount *= (1 + rate)\n",
    "\n",
    "# 結果を表示（小数点以下2桁）\n",
    "print(f\"10年後の金額: {amount:.2f}\")\n"
   ]
  }
